@using Car;
<!-- RazorPage -->
@{
    FourWheels fw = ViewData["passcar"] as FourWheels;
}
<div class="text-center">
    <h1>@ViewData["Title"] Bugatti Chiron</h1>
    <hr>
    <img src= "@fw.imageurl" alt="Chiron" height="300px" width="450px"/>
    @* <img src="/Images/frontangle.webp" alt="Chiron" height="210px" width="280px"/>
    <img src="/Images/backside.webp" alt="Chiron" height="210px" width="280px"/> *@
    <hr/>
    <p>Name: @fw.prodname<p/>
    <p>Company: @fw.prodcompany<p/>
    <p>Engine: @fw.prodengine<p/>
    <p>Cost: @fw.prodcost<p/>
    <p>Quantity: @fw.quantity<p/>
    <hr>
    <button>Order</button>
</div>